<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="93.0" prefWidth="256.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="archivocsv.controllers.VentanaMesController">
   <children>
      <Label layoutX="21.0" layoutY="22.0" text="Mes">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="txtMes" layoutX="56.0" layoutY="20.0" prefHeight="25.0" prefWidth="184.0" promptText="Ingrese un mes" />
      <Button layoutX="109.0" layoutY="60.0" mnemonicParsing="false" onAction="#aceptarMes" text="Aceptar">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Lighting diffuseConstant="2.0" specularConstant="0.0" specularExponent="0.0" surfaceScale="1.63">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant azimuth="213.48" color="#9eddffa1" />
               </light>
            </Lighting>
         </effect>
      </Button>
      <Button layoutX="178.0" layoutY="60.0" mnemonicParsing="false" onAction="#cancelarMes" text="Cancelar">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Lighting diffuseConstant="2.0" specularConstant="0.0" specularExponent="0.0" surfaceScale="1.63">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant azimuth="213.48" color="#9eddffa1" />
               </light>
            </Lighting>
         </effect>
      </Button>
   </children>
</AnchorPane>
